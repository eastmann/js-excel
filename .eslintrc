{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "semi": "off",
        "comma-dangle": "off",
        "linebreak-style": "off",
        "arrow-parens": "off",
        "indent": ["error", 4],
        "require-jsdoc": "off",
        "object-curly-spacing": ["error", "always"]
    },
    "extends": [
        "eslint:recommended",
        "google"
    ]
}
